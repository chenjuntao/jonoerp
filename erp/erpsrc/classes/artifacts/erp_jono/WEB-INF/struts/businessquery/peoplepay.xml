<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="bq_peoplepay" namespace="/businessquery/peoplepay" extends="trust-default">
		<action name="mainView" class="action.businessquery.PeoplePayAction">
			<result name="success">/jsp/businessquery/peoplepay/main.jsp</result>
		</action>

		<action name="getData" method="getData" class="action.businessquery.PeoplePayAction">
			<result type="json" />
		</action>
		
		<action name="getColumn" method="getColumn" class="action.businessquery.PeoplePayAction">
			<result type="json" />
		</action>
		
		<action name="getPeople" method="getPeople" class="action.businessquery.PeoplePayAction">
			<result type="json" />
		</action>
	</package>
</struts>

