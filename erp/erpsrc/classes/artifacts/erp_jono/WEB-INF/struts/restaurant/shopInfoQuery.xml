<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<!--     餐厅基本信息查询 -->
	<package name="restaurant_shopInfoQuery" namespace="/restaurant/shopInfoQuery"	extends="trust-default">
		<action name="listShopInfo" method="listShopInfo" class="action.restaurant.ShopInfoAction">
			<result type="json" />
		</action>

		<action name="mainView" class="action.restaurant.ShopInfoAction">
			<result name="success">/jsp/restaurant/shopinfo/main.jsp</result>
		</action>
	</package>
</struts>

