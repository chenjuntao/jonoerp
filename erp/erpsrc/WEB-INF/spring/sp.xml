<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop = "http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
	http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-2.5.xsd"
	default-autowire="byName">

	<bean id="supplierScanBean" class="logic.module.supplier.SupplierScanBean" />
	<bean id="supplierInScanBean" class="logic.module.supplier.SupplierInScanBean" />
	<bean id="supplierReturnBean" class="logic.module.supplier.SupplierReturnBean" />
	
	<bean id="supplierStatement" class="logic.module.supplier.SupplierStatement" />
	<bean id="supplierService" class="service.supplier.SupplierService" />
</beans>

