<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="setting_role" namespace="/setting/role" extends="trust-default">
		<action name="mainView" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result name="success">/jsp/setting/role/main.jsp</result>
		</action>
		
		<action name="addView" method="editView" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result name="success">/jsp/setting/role/add.jsp</result>
		</action>
		
		<action name="editView" method="editView" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result name="success">/jsp/setting/role/edit.jsp</result>
		</action>
		
		<action name="checkRoleId" method="checkRoleId" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result type="json" />
		</action>
		
		<action name="doQuery" method="doQuery" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result type="json" />
		</action>
		
		<action name="doSave" method="doSave" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result type="json" />
		</action>
		
		<action name="deletable" method="deletable" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result type="json" />
		</action>
		
		<action name="doDelete" method="doDelete" class="com.tanry.business.module.setting.user.action.RoleManageAction">
			<result type="json" />
		</action>
	</package>
</struts>

